{
  "manifest_version": 3,
  "name": "CacheCleaner",
  "version": "2.1",
  "description": "Extention that manages storage and clears up cache",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://web.telegram.org/*"]
    }
  ]
}
